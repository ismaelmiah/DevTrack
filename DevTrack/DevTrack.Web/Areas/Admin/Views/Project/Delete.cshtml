@model DevTrack.Web.Areas.Admin.Models.ProjectCreateModel

@{
    ViewData["Title"] = "Delete";
}


    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            <h1>Are you sure you want to delete this?</h1>
            <hr />

            <dl class="row">
                <dt class="col-sm-4">
                    Project Id
                </dt>
                <dd class="col-sm-8">
                    @Model.Id
                </dd>
                <dt class="col-sm-4">
                    Project Name
                </dt>
                <dd class="col-sm-8">
                    @Model.Name
                </dd>
            </dl>


            <form asp-action="Delete">
                <input type="submit" value="Delete" class="btn btn-danger" />
                <a asp-action="Index" class="btn btn-primary">Cancel</a>
            </form>

        </div>  
    </div>


